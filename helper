# Your Python dictionary
data = {
    'name': ['John', 'Jane', 'Bob'],
    'age': [28, 35, 40],
    'city': ['New York', 'San Francisco', 'Los Angeles']
}

# Convert the dictionary to a Spark DataFrame
df = spark.createDataFrame(list(zip(data['name'], data['age'], data['city'])), 
                            schema=['name', 'age', 'city'])
